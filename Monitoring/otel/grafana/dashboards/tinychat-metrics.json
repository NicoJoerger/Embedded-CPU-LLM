{
  "uid": "tinychat-metrics",
  "title": "TinyChatEngine Token Generation Metrics",
  "timezone": "browser",
  "schemaVersion": 38,
  "version": 1,
  "refresh": "5s",
  "panels": [
    {
      "type": "timeseries",
      "title": "Tokens per Second",
      "datasource": "InfluxDB",
      "targets": [
        {
          "query": "from(bucket: \"llm\")\n  |> range(start: v.timeRangeStart, stop: v.timeRangeStop)\n  |> filter(fn: (r) => r._measurement == \"token_cumulative\")\n  |> filter(fn: (r) => r._field == \"tokens_per_second\")\n  |> map(fn: (r) => ({ r with _value: r._value, series: r.request_id }))",
          "refId": "A"
        }
      ],
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 0 },
      "fieldConfig": {
        "defaults": {
          "unit": "none",
          "custom": {
            "axisLabel": "Tokens/s",
            "drawStyle": "line",
            "lineInterpolation": "linear",
            "lineWidth": 2,
            "fillOpacity": 10,
            "showPoints": "auto"
          }
        }
      },
      "options": {
        "legend": {
          "displayMode": "table",
          "placement": "bottom",
          "showLegend": true,
          "calcs": ["last", "max", "mean"]
        }
      }
    },
    {
      "type": "timeseries",
      "title": "Average Token Time (ms)",
      "datasource": "InfluxDB",
      "targets": [
        {
          "query": "from(bucket: \"llm\")\n  |> range(start: v.timeRangeStart, stop: v.timeRangeStop)\n  |> filter(fn: (r) => r._measurement == \"token_cumulative\")\n  |> filter(fn: (r) => r._field == \"avg_token_time_ms\")\n  |> map(fn: (r) => ({ r with series: \"avg_with_ttft_\" + r.request_id }))",
          "refId": "A"
        },
        {
          "query": "from(bucket: \"llm\")\n  |> range(start: v.timeRangeStart, stop: v.timeRangeStop)\n  |> filter(fn: (r) => r._measurement == \"token_cumulative\")\n  |> filter(fn: (r) => r._field == \"avg_token_time_no_ttft_ms\")\n  |> map(fn: (r) => ({ r with series: \"avg_no_ttft_\" + r.request_id }))",
          "refId": "B"
        }
      ],
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 0 },
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
          "custom": {
            "axisLabel": "Time (ms)",
            "drawStyle": "line",
            "lineInterpolation": "linear",
            "lineWidth": 2,
            "fillOpacity": 10,
            "showPoints": "auto"
